<toolbox>

    <!-- loop() 카테고리 안에 들어갈 블록   -->
    <category name="drone_blocks" colour="120">


        <block type="arm_start"></block>
        <block type="disarm_start"></block>
        <block type="drone_delay">
            <field name="NUM">5</field>
        </block>
        <block type="take_off"></block>
        <block type="land"></block>

        <block type="drone_for">
            <value name="FROM">
                <block type="math_number">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>

        <block type="go">
            <field name="NUM">30</field>
        </block>

        <block type="back">
            <field name="NUM">30</field>
        </block>

        <block type="left">
            <field name="NUM">30</field>
        </block>

        <block type="right">
            <field name="NUM">30</field>
        </block>

        <block type="up">
            <field name="NUM">30</field>
        </block>

        <block type="down">
            <field name="NUM">30</field>
        </block>


        <block type="ccw">
            <field name="NUM">30</field>
        </block>

        <block type="cw">
            <field name="NUM">30</field>
        </block>




    </category>

    <category name="drone_cali" colour="330">

    </category>

    <category name="drone_serial" colour="330">

